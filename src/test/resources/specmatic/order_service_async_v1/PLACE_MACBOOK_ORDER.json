{
  "receive": {
    "topic": "place-order",
    "headers": {
      "requestId": 200
    },
    "payload": {
      "id": 1,
      "orderItems": [
        {
          "id": 1,
          "name": "Macbook",
          "quantity": 50,
          "price": 6000.00
        }
      ]
    }
  },
  "send" : [
    {
      "topic": "process-order",
      "headers": {
        "requestId": 200
      },
      "payload": {
        "totalAmount": 300000.00,
        "status": "PROCESSED"
      }
    }
  ]
}
