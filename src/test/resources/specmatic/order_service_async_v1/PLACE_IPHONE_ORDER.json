{
  "receive": {
    "topic": "place-order",
    "headers": {
      "requestId": "100"
    },
    "payload": {
      "id": 1,
      "orderItems": [
        {
          "id": 1,
          "name": "iPhone",
          "quantity": 10,
          "price": 5000.00
        }
      ]
    }
  },
  "send" : [
    {
      "topic": "process-order",
      "headers": {
        "requestId": 100
      },
      "payload": {
        "totalAmount": 50000.00,
        "status": "PROCESSED"
      }
    }
  ]
}
