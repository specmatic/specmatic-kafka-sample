{
  "publish": {
    "topic": "place-order",
    "payload": {
      "orderItems": [
        {
          "id": 1,
          "name": "iPhone",
          "quantity": 10,
          "price": 5000
        }
      ]
    }
  },
  "subscribe" : [
    {
      "topic": "process-order",
      "payload": {
        "id": 1,
        "totalAmount": 50000,
        "status": "PROCESSED"
      }
    },
    {
      "topic": "notification",
      "payload": {
        "message": "Order processed successfully",
        "type": "ORDER_PLACED"
      }
    }
  ]
}
